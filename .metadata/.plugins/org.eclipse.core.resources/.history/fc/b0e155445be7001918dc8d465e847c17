package com.bridgelab.functions;

public class dayOfWeek {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int month, year,day;
		String []da= {"sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"};
		day  = Integer.parseInt(args[0]);
    	month = Integer.parseInt(args[1]);
       	year = Integer.parseInt(args[2]);
       	int d=utility.dayofweek(month, day, year);
       	System.out.println("entered date is "+day +"/"+month+"/"+year);
       	System.out.println("The day of week is "+da[d]);
	}

}
